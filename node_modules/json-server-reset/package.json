{
  "name": "json-server-reset",
  "description": "Reset middleware for json-server",
  "version": "1.0.0",
  "author": {
    "name": "Gleb Bahmutov",
    "email": "gleb.bahmutov@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/bahmutov/json-server-reset/issues"
  },
  "config": {
    "pre-git": {
      "commit-msg": "simple",
      "pre-commit": [
        "npm prune",
        "npm run deps",
        "npm test",
        "git add src/*.js",
        "npm run ban"
      ],
      "pre-push": [
        "npm run unused-deps",
        "npm run secure",
        "npm run license",
        "npm run ban -- --all",
        "npm run size"
      ],
      "post-commit": [],
      "post-merge": []
    }
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "src/*.js",
    "!src/*-spec.js"
  ],
  "homepage": "https://github.com/bahmutov/json-server-reset#readme",
  "keywords": [
    "json-server",
    "middleware",
    "reset"
  ],
  "license": "MIT",
  "main": "src/",
  "private": false,
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahmutov/json-server-reset.git"
  },
  "scripts": {
    "ban": "ban",
    "deps": "deps-ok && dependency-check --no-dev .",
    "issues": "git-issues",
    "license": "license-checker --production --onlyunknown --csv",
    "lint": "standard --verbose --fix src/*.js",
    "prelint": "echo disabled npm run pretty",
    "pretest": "npm run lint",
    "pretty": "prettier-standard 'src/*.js'",
    "secure": "nsp check",
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";",
    "test": "npm run unit",
    "unit": "mocha src/*-spec.js",
    "unused-deps": "dependency-check --unused --no-dev .",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "start": "json-server cypress/fixtures/data.json --middlewares ./src/index.js",
    "e2e": "start-server-and-test start http://localhost:3000 cy:run",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "release": {
    "analyzeCommits": "simple-commit-message",
    "generateNotes": "github-post-release",
    "verifyRelease": {
      "path": "dont-crack",
      "test-against": []
    }
  },
  "devDependencies": {
    "ban-sensitive-files": "1.9.2",
    "check-more-types": "2.24.0",
    "cypress": "1.1.2",
    "dependency-check": "2.9.1",
    "deps-ok": "1.2.1",
    "dont-crack": "1.2.1",
    "git-issues": "1.3.1",
    "github-post-release": "1.13.1",
    "json-server": "0.12.1",
    "lazy-ass": "1.6.0",
    "license-checker": "15.0.0",
    "mocha": "4.0.1",
    "nsp": "3.1.0",
    "pre-git": "3.16.0",
    "prettier-standard": "7.0.3",
    "semantic-release": "8.2.0",
    "simple-commit-message": "3.3.2",
    "standard": "10.0.3",
    "start-server-and-test": "1.0.0"
  },
